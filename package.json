{
  "name": "headlines",
  "version": "1.0.0",
  "description": "A news headlines app built with React",
  "main": "server.js",
  "scripts": {
    "test": "nyc karma start",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "webpack",
    "start": "npm run build && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codejockie/headlines.git"
  },
  "keywords": [
    "headline",
    "news"
  ],
  "author": "John Kennedy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codejockie/headlines/issues"
  },
  "homepage": "https://github.com/codejockie/headlines#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.3",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "firebase": "^4.0.0",
    "flux": "^3.1.2",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "node-env-file": "^0.1.8",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^3.0.0",
    "react-router-dom": "^4.1.1",
    "sass-loader": "^6.0.5",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.1",
    "webpack": "^2.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-register": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "webpack-dev-server": "^2.4.5"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "app/components/*.jsx"
    ],
    "exclude": [
      "app/tests/*.test.jsx"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-plugin-__coverage__",
      "babel-register",
      "mocha"
    ],
    "extension": [
      ".jsx",
      ".es6"
    ],
    "cache": true,
    "all": true,
    "report-dir": ".nyc_output"
  }
}
